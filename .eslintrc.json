{
  "rules": {
    "no-console": ["error", {"allow":["error", "warn", "info"]}],
    "no-multiple-empty-lines": ["warn", {"max": 1}],
    "no-undef": 2,
    "quotes": ["error", "single"]
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": "eslint:recommended"
}
